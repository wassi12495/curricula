<h1>Create New Book</h1>


<ul>
<%= form_for @book do |f|%>

<%= f.label :title%>
<%= f.text_field :title%>
<br>
<%= f.label :author%>
<%= f.text_field :author%>
<br>
<%= f.label :year%>
<%= f.text_field :year%>
<br>
<%= f.label :publisher%>
<%= f.text_field :publisher%>
<br>
<%= f.label :description%>
<%= f.text_area :description%>
<br>
<%= f.label :subject %>
<%= f.collection_select(:subject_id, Subject.all, :id, :name)%>

<br>
<%= f.submit %>
<%end%>



</ul>
